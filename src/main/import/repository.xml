<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
   <modules jcr:primaryType="jnt:modules">
      <restaurant j:dependencies="default external-provider-users-groups graphql-dxm-provider location rating"
                  j:modulePriority="0"
                  j:moduleType="module"
                  j:title="restaurant"
                  jcr:primaryType="jnt:module">
         <portlets jcr:primaryType="jnt:portletFolder"/>
         <files jcr:primaryType="jnt:folder"/>
         <contents jcr:primaryType="jnt:contentFolder"/>
         <templates j:rootTemplatePath="/base" jcr:primaryType="jnt:templatesFolder">
            <files jcr:primaryType="jnt:folder"/>
            <contents jcr:primaryType="jnt:contentFolder"/>
            <details-template j:applyOn="jc:restaurant"
                              j:defaultTemplate="true"
                              j:hiddenTemplate="false"
                              j:view="default"
                              jcr:primaryType="jnt:contentTemplate">
               <j:translation_en jcr:language="en"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="details-template"/>
               <pagecontent jcr:mixinTypes="jmix:systemNameReadonly"
                            jcr:primaryType="jnt:contentList">
                  <back jcr:primaryType="jnt:backToParentPage">
                     <j:translation_en jcr:language="en"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Back"/>
                     <j:translation_de jcr:language="de"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Back"/>
                     <j:translation_pt jcr:language="pt"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Back"/>
                     <j:translation_it jcr:language="it"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Back"/>
                     <j:translation_fr jcr:language="fr"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Back"/>
                     <j:translation_es jcr:language="es"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Back"/>
                  </back>
                  <main-resource-display j:mainResourceView="detail" jcr:primaryType="jnt:mainResourceDisplay"/>
                  <map-static j:height="300"
                              j:imageType="png8"
                              j:mapType="roadmap"
                              j:markerColor="normal"
                              j:markerSize="normal"
                              j:view="default"
                              j:width="500"
                              j:zoom="auto"
                              jcr:mixinTypes="jmix:renderable"
                              jcr:primaryType="jnt:staticMap"/>
                  <rating jcr:primaryType="jnt:rateable"/>
               </pagecontent>
            </details-template>
         </templates>
      </restaurant>
   </modules>
</content>